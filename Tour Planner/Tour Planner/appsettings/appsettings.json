{
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=tourdb;Username=postgres;Password=Debian123!"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning"
    }
  },
  "OpenRouteService": {
    "ApiKey": "your-api-key-here"
  },
  "log4net": {
    "appender": {
      "RollingFileAppender": {
        "file": "log-file.log",
        "appendToFile": true,
        "maximumFileSize": "10MB",
        "maxSizeRollBackups": 5,
        "layout": {
          "conversionPattern": "%date{ISO8601} [%thread] %-5level %logger - %message%newline"
        }
      }
    },
    "root": {
      "level": "DEBUG",
      "appender-ref": {
        "ref": "RollingFileAppender"
      }
    }
  }
}
